#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ros2-mpu6050==0.0.1','console_scripts','mpu6050'
__requires__ = 'ros2-mpu6050==0.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ros2-mpu6050==0.0.1', 'console_scripts', 'mpu6050')()
    )
